<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zt.dao.mapper.SelectpackMapperCustom" >
	
<select id="findAll" parameterType="com.zt.pojo.vo.SelectpackQueryVo" resultType="com.zt.pojo.po.TSelectpack">
	select * from t_selectPack 
</select>

 <select id="count" parameterType="com.zt.pojo.vo.SelectpackQueryVo" resultType="int">
  	select count(id) from t_selectPack
  	<include refid="com.zt.dao.mapper.Common.pageQuery"/>
 </select>
 
<update id="update" parameterType="com.zt.pojo.po.TSelectpack">
	update  t_selectPack set status =${status} where id=#{id}
</update>

<select id="findId" parameterType="int" resultType="com.zt.pojo.po.TSelectpack">
	select *from t_selectPack where id=${_parameter}
</select>


</mapper>