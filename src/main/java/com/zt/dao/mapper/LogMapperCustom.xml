<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zt.dao.mapper.LogMapperCustom" >
	<insert id="insert" parameterType="com.zt.pojo.po.LogCustom">
		insert into ${tablename} (operationId,usersId,usersName,createTime,content)
		value(#{operationid,jdbcType=INTEGER},#{userid,jdbcType=INTEGER},#{username,jdbcType=VARCHAR},now(),#{content,jdbcType=VARCHAR}
		)
	</insert>
</mapper>